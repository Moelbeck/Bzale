<?xml version="1.0"?>
<configuration>
  <!--<appSettings>
    <add key="aspnet:UseTaskFriendlySynchronizationContext" value="true" />
  </appSettings>-->
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.5.1" />
      </system.Web>
  -->
  <system.web>
    <compilation debug="true" targetFramework="4.5.2"/>
    <!--<httpRuntime targetFramework="4.5.2"/>-->
    <pages controlRenderingCompatibilityVersion="4.0"/>
  </system.web>
  <system.serviceModel>
    <bindings>
      <wsHttpBinding>
        <binding name="TransportSecurity">
          <security mode="Transport">
            <transport clientCredentialType="Basic"/>
          </security>
        </binding>
      </wsHttpBinding>
    </bindings>
    <services>
      <service behaviorConfiguration="" name="bzale.WcfWebService.CategoryService">
        <endpoint address="" binding="basicHttpBinding" contract="bzale.WcfWebService.ICategoryService"/>
        <endpoint address="mex" binding="mexHttpBinding" contract="bzale.WcfWebService.ICategoryService"/>
      </service>
      <service behaviorConfiguration="" name="bzale.WcfWebService.RatingService">
        <endpoint address="" binding="basicHttpBinding" contract="bzale.WcfWebService.IRatingService"/>
        <endpoint address="mex" binding="mexHttpBinding" contract="bzale.WcfWebService.IRatingService"/>
      </service>
      <service behaviorConfiguration="" name="bzale.WcfWebService.SaleListingService">
        <endpoint address="" binding="basicHttpBinding" contract="bzale.WcfWebService.ISaleListingService"/>
        <endpoint address="mex" binding="mexHttpBinding" contract="bzale.WcfWebService.ISaleListingService"/>
      </service>
      <service behaviorConfiguration="" name="bzale.WcfWebService.AccountService">
        <endpoint address="" binding="basicHttpBinding" contract="bzale.WcfWebService.IAccountService" />
        <endpoint address="mex" binding="mexHttpBinding" contract="bzale.WcfWebService.IAccountService"/>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior>
          <serviceMetadata httpGetEnabled="true"/>
          <serviceDebug includeExceptionDetailInFaults="false"/>
        </behavior>
        <behavior name="sslbehavior">
          <serviceMetadata httpGetEnabled="true"/>
          <serviceDebug includeExceptionDetailInFaults="true"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <!--<protocolMapping>
        <add binding="basicHttpsBinding" scheme="https" />
    </protocolMapping>-->
    <serviceHostingEnvironment multipleSiteBindingsEnabled="true"/>
  </system.serviceModel>
  <system.webServer>
    <modules runAllManagedModulesForAllRequests="true"/>
    <directoryBrowse enabled="true"/>
  </system.webServer>
</configuration>